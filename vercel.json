{
    "version": 2,
    "name": "petgram",
    "routes": [
        {
            "src": "(.*).js",
            "dest": "/$1.js"
        },
        {
            "src": "(.*).json",
            "dest": "/$1.json"
        },
        {
            "src": "/.*",
            "dest": "/index.html"
        },
        {
            "src": "/(.*).png",
            "dest": "/$1.png"
        }
    ],
    "builds": [{ "src": "index.js", "use": "@now/node" }]
}
